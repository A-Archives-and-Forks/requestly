  "name": "@requestly/session-bear",
  "version": "24.5.27",
  "description": "Capture HTTP Sessions and kill bugs in seconds",
  "homepage": "https://requestly.com",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/requestly/requestly.git",
